<p id="notice"><%= notice %></p>

<h1>Attendances</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>User.id</th>
      <th>Course.id</th>
      <th>Archived</th>
    </tr>
  </thead>

  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= attendance.user_id %></td>
        <td><%= link_to attendance.course_id, attendance.course %></td>
        <td><%= attendance.archived %></td>
      </tr>
    <% end %>
  </tbody>
</table>

