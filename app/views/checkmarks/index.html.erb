<p id="notice"><%= notice %></p>

<h1>Checkmarks</h1>

<table>
  <thead>
    <tr>
      <th>user_id</th>
      <th>exercise_id</th>
      <th>status</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @checkmarks.each do |checkmark| %>
      <tr>
        <td><%= checkmark.user_id %></td>
        <td><%= checkmark.exercise_id %></td>
        <td><%= checkmark.status %></td>
        <td><%= link_to 'delete', checkmark, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

